{
	"compilerOptions": {
	  "target": "es2018",
	  "module": "commonjs",
	  "lib": ["es2018"],
	  "declaration": true,
	  "outDir": "./dist",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": [
	  "src/**/*.ts"
	],
	"exclude": [
	  "node_modules",
	  "dist"
	]
  }
  